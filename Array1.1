class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int n = nums.size();
        vector<int> ans(n);
        ans[0] = 1;
        int rp =1;
        for(int i = 1; i < n; i ++){
            rp *= nums[i-1];
            ans[i] = rp;
        }
         rp =1;
        
        for( int i = n-2 ; i >=0 ; i--){
            rp *= nums[i+1];
            cout<<rp<<endl;
            cout<<ans[i]<<"ans"<<endl;
            ans[i] = rp*ans[i];
            
        }
        return ans;
    }
};
